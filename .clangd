  -Wextra, -Wpedantic, -Wshadow, -Wconversion, -Wfloat-equal, -Wformat-nonliteral, -Wformat-security, -Wformat-y2k,
  -Wformat=2, -Wimport, -Winvalid-pch, -Wlogical-op, -Wmissing-declarations, -Wmissing-field-initializers,
  -Wmissing-format-attribute, -Wmissing-include-dirs, -Wmissing-noreturn,  -Wpacked, -Wpointer-arith, -Wredundant-decls,
  -Wstack-protector, -Wstrict-null-sentinel, -Wswitch-enum, -Wundef, -Wwrite-strings]
